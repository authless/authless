{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "strict": true,
    "noImplicitAny": false,
    "declaration": true,
    "removeComments": false,
    "declarationMap": true,
    "preserveConstEnums": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "outDir": "./dist"
  },
  "include": ["**/**/*.ts"],
  "exclude": [
    "node_modules",
    "./dist/**/**"
  ]
}
